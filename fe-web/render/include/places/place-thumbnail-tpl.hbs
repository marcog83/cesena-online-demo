<div class="place-thumbnail" itemscope itemtype="http://schema.org/Place">
    <a href="{{url}}" itemprop="url">
        <div class="image" style="background-image: url('{{image}}')">
            <meta itemprop="photo" content="{{image}}"/>
        </div>
        {{> include/places/place-info }}
    </a>
</div>